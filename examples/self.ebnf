grammar ::=
  rule+

rule ::=
  identifier "::=" newline {rule_entry newline}+ newline+

rule_entry ::=
  rule_token+

rule_token ::=
  string modifier?
  identifier modifier?
  "Îµ" modifier?
  "{" rule_entry "}" modifier?

identifier ::=
  /[a-zA-Z_][a-zA-Z_0-9]*/

newline ::=
  "\n"

string ::=
  /"([^\\"\n]|\\.)*"/

regex ::=
  /\/([^\\\/\n]|\\.)*\//

modifier ::=
  /[+?*]/

